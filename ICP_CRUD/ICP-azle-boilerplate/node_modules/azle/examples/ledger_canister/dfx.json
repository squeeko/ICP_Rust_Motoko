{
    "canisters": {
        "ledger_canister": {
            "type": "custom",
            "main": "src/ledger_canister/index.ts",
            "candid": "src/ledger_canister/index.did",
            "build": "npx azle ledger_canister",
            "wasm": ".azle/ledger_canister/ledger_canister.wasm",
            "gzip": true,
            "declarations": {
                "output": "test/dfx_generated/ledger_canister",
                "node_compatibility": true
            },
            "env": ["ICP_CANISTER_PRINCIPAL", "AZLE_TEST_FETCH"],
            "assets": [["src/icp_ledger/ledger.public.did", "src/ledger.did"]],
            "metadata": [
                {
                    "name": "candid:service",
                    "path": "src/ledger_canister/index.did"
                },
                {
                    "name": "cdk:name",
                    "content": "azle"
                }
            ]
        },
        "icp_ledger": {
            "type": "custom",
            "candid": "src/icp_ledger/ledger.private.did",
            "wasm": "src/icp_ledger/ledger.wasm"
        }
    }
}
